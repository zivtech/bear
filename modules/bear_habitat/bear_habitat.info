name = Bear Habitat
description = Contains Bear configuration settings for different server environments (dev, testing, local) using the Habitat module.
core = 7.x
package = Bear
project = bear
dependencies[] = ctools
dependencies[] = habitat
dependencies[] = strongarm
features[ctools][] = strongarm:strongarm:1
features[features_api][] = api:2
features[variable][] = habitat_disable_dev
features[variable][] = habitat_disable_local
features[variable][] = habitat_disable_prod
features[variable][] = habitat_disable_test
features[variable][] = habitat_enable_dev
features[variable][] = habitat_enable_local
features[variable][] = habitat_enable_prod
features[variable][] = habitat_enable_test
features[variable][] = habitat_habitats
features[variable][] = habitat_variable
